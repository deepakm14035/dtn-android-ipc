<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mysignal">
    <uses-permission android:name="android.permission.messagingproviderreadperm"></uses-permission>
    <uses-permission android:name="android.permission.messagingproviderwriteperm"></uses-permission>
    <uses-permission android:name="android.permission.messagingproviderwriteperm"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MySignal">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <queries>
        <provider android:authorities="com.example.contentprovidertest.providers" />
    </queries>

</manifest>